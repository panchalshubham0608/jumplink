{
    "manifest_version": 3,
    "name": "JumpLink",
    "description": "Instantly navigate with custom short URLs. Simplify your browsing with a quick `jump <keyword>`",
    "version": "1.0.1",
    "permissions": [
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "options_page": "options.html",
    "action": {
        "default_popup": "options.html"
    },
    "icons": {
        "16": "jumplink.png",
        "48": "jumplink.png",
        "128": "jumplink.png"
    },
    "omnibox": {
        "keyword": "jump"
    }
}
